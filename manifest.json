{
  "name": "Memento Mori",
  "author": "mance-rah",
  "description": "Reminds the user of the brevity of life with every new tab.",
  "version": "0.1",
  "manifest_version": 2,

  "icons": {
    "16": "img/age-16.png",
    "48": "img/age-48.png",
    "128": "img/age-128.png"
  },

  "background": {
    "persistent": false,
    "scripts": [
      "db/birthdate.js",
      "mementomori.js"
    ]
  },

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "web_accessible_resources": [
    "img/*",
    "js/*",
    "css/*"
  ]
}